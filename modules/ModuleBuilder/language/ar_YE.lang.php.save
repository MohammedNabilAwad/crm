    'assistantHelp' => array(
        'package' => array(
            //custom begin
            'nopackages' => 'لبدء مشروع ما، انقر على <b>حزمة جديدة</b> لإنشاء حزمة جديدة لإحتواء وحدة أو وحدات مخصصة خاصة بك. <br/><br/>يمكن أن تحتوي كل حزمة على وحدة واحدة أو أكثر.<br/><br/>على سبيل المثال، قد ترغب في إنشاء حزمة تحتوي على وحدة مخصصة ذات صلة بوحدة الحسابات القياسية. أو قد ترغب في إنشاء حزمة تحتوي على العديد من الوحدات الجديدة التي تعمل معا كمشروع والتي ترتبط بعضها بالبعض وبوحدات أخرى موجودة بالفعل في النظام.',
            'somepackages' => 'تعمل <b>الحزمة</b> كحاوية لوحدات مخصصة كلها جزء من مشروع واحد. يمكن أن تحتوي الحزمة على واحدة أو أكثر من <b>الوحدات</b> المخصصة التي يمكن أن تكون مرتبطة ببعضها البعض أو بوحدات أخرى في النظام.<br/><br/>بعد إنشاء حزمة لمشروعك، يمكنك إنشاء وحدات للحزمة على الفور، أو يمكنك العودة إلى إداة إنشاء الوحدات في وقت لاحق لإكمال المشروع.<br><br>عند اكتمال المشروع، يمكنك <b>تنفيذ</b> الحزمة لتثبيت الوحدات المخصصة داخل التطبيق.',
            'afterSave' => 'يجب أن تحتوي الحزمة الجديدة الخاصة بك على وحدة واحدة على الأقل. يمكنك إنشاء وحدة أو أكثر من الوحدات المخصصة في الحزمة.<br/><br/>انقر على <b>وحدة جديدة</b> لإنشاء وحدة مخصصة في هذه الحزمة.<br/><br/> بعد إنشاء وحدة واحدة على الأقل, يمكنك نشر أو تنفيذ الحزمة لجعلها متاحة لنسخة المستخدم الخاصة بك و/أو لنسخ المستخدمين الآخرين.<br/><br/> لتنفيذ الحزمة في خطوة واحدة داخل نسخة SuiteCRM الخاصة بك، انقر على <b>تنفيذ</b>.<br><br>اضغط <b>نشر</b> لحفظ الحزمة كملف zip. بعد ذلك يتم حفظ الملف المضغوط zip في النظام الخاص بك، استخدم <b>أداة رفع الوحدات</b> لرفع وتثبيت الحزمة داخل نسخة SuiteCRM الخاصة بك. <br/><br/>يمكنك توزيع الملف على المستخدمين الآخرين لتحميله وتثبيته داخل نسخ SuiteCRM الخاصة بهم.',
            'create' => 'تعمل <b>الحزمة</b> كحاوية لوحدات مخصصة كلها جزء من مشروع واحد. يمكن أن تحتوي الحزمة على واحدة أو أكثر من <b>الوحدات</b> المخصصة التي يمكن أن تكون مرتبطة ببعضها البعض أو بوحدات أخرى في النظام.<br/><br/>بعد إنشاء حزمة لمشروعك، يمكنك إنشاء وحدات للحزمة على الفور، أو يمكنك العودة إلى إداة إنشاء الوحدات في وقت لاحق لإكمال المشروع.',
        ),
        'main' => array(
            'welcome' => 'استخدم <b>أدوات المطور</b> لإنشاء وإدارة وحدات وحقول قياسية ومخصصة. <br/><br/>لإدارة الوحدات في النظام، انقر على <b>الاستوديو</b>. <br/><br/>لإنشاء وحدات مخصصة، انقر <b>أداة بناء الوحدات</b>.',
            'studioWelcome' => 'جميع الوحدات المثبتة حالياً، بما في ذلك العناصر المعيارية والمحملة من خلال الوحدات، قابلة للتخصيص داخل الاستوديو.'
